# defining two sets
E = {0, 2, 4, 6, 8}
N = {1, 2, 3, 4, 5}
 
# printing union using |
print("Union of E and N is",E | N)
 
# printing intersection using &
print("Intersection of E and N is",E & N)
 
# printing difference using -
print("Difference of E and N is",E - N)
 
# printing symmetric difference using ^
print("Symmetric difference of E and N is",E ^ N)
